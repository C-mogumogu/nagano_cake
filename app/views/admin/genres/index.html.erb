<h3>ジャンル一覧・追加</h3>
<table>
  <tr>ジャンル名</tr>
  <tr><%= form_with model: @name %></tr>
</table>
<table class="table table-hover tabke-inverse">

  <thead>
    <tr>
      <th>ジャンル</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @names.each do |name| %>
      <tr>
        <td><%= name %></td>
      </tr>
    <% end %>
  </tbody>

</table>