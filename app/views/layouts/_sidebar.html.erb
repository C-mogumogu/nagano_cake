<div class="card text-dark bg-light">
  <div class="card-header text-center">ジャンル検索</div>
  <div class="card-body">
    <ul class="card-text list-unstyled">
      <% genres.each do |genre| %>
        <li><%= link_to genre.name, genre_search_path(genre.id) %></li>
      <% end %>
    </ul>
  </div>
</div>
